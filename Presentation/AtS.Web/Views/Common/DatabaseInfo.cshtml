
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DatabaseInfo</title>
    <link href="~/Content/css/styles.css" rel="stylesheet" />
    <script src="~/Content/js/jquery-3.4.1.min.js"></script>
    <script src="~/Content/css/bootstrap.bundle.min.js"></script>
</head>
<body class="d-flex flex-column min-vh-100">

<header class="p-3 bg-dark text-white">
    <div class="container">
        <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
            <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none">
                <svg class="bi me-2" width="40" height="32" role="img" aria-label="Bootstrap"><use xlink:href="#bootstrap"></use></svg>
            </a>
        </div>
    </div>
</header>

<div class="container col-xxl-8 px-4 py-5">
    <div class="row flex-lg-row-reverse align-items-center g-5 py-5">
        <div class="col-10 col-sm-8 col-lg-6">
            <img src="~/Content/Images/data-missing-creative.svg" class="d-block mx-lg-auto img-fluid" alt="Bootstrap Themes" loading="lazy" width="700" height="500">
        </div>
        <div class="col-lg-8">

            <h1 class="display-5 fw-bold lh-1 mb-3">It looks like you have not installed the database correctly.</h1>

            <p class="lead">Detailed instructions can be found in the <strong >Readme</strong> file in the project repository.</p>

           
        </div>
    </div>
</div>

    @Html.Partial("_Footer")
</body>
</html>
