@model List<TWYK.Web.Models.CategoryModel>
@{
    ViewBag.Title = "Home Page";
}

@foreach (var categoryModel in Model) {
    <div class="row justify-content-center">
        <div class="row">
            <h2>Best of <strong>@categoryModel.Name</strong> Shoes</h2>
        </div>

        @foreach (var prd in categoryModel.Products) {
            @Html.Partial("_ProductBox", prd)
        }
    </div>
}