@model List<TWYK.Web.Models.CategoryModel>
@{
    ViewBag.Title = "Product List";
}
<h2>All Products</h2>
<div class="row">
    <div class="col-2">
        @Html.Action("SideBarMenu", "Catalog" , new {categoryId = 0})
    </div>
    <div class="col-10">
        <div class="row gx-4 gx-lg-5 row-cols-2 row-cols-md-3">
            @foreach (var categoryModel in Model) {
                foreach (var prd in categoryModel.Products) {
                    @Html.Partial("_ProductBox", prd)
                }
            }
        </div>
    </div>
</div>


